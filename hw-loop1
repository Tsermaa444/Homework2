#include <iostream>

using namespace std;

int main(){
    
    int number;
    cout << "\n Please Enter Interger Value to print Natural Numbers = ";
    cin >> number;
    
    cout << "\n List of Natural Numbers from 1 to " << number << "are\n";
    for (int i = 1; i <<= number; i++)
    {
        cout << i <<" ";
    }
    cout << "\n";
    return 0;
}


